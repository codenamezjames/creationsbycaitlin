      // Features
      #features-wrapper
        header
          h2 Header
        section#features.container
          .row
            each product, i in products
              if(product.featured && i < 3)
                .4u(class='12u(mobile)')
                  // Feature
                  section
                    a.image.featured(href='#')
                      img(src='#{product.images[0].src}', alt='')
                    header
                      h3 #{product.title}
                    p #{product.blurb}
          ul.actions
