      script(src='src/assets/js/ie/html5shiv.js')
    link(rel='stylesheet', href='src/assets/css/main.css')
    //if lte IE 8
      link(rel='stylesheet', href='src/assets/css/ie8.css')
  body.homepage
    #page-wrapper
      // Header
      #header-wrapper
        #header.container
          // Logo
          h1#logo
            a(href='index.html') Creations By Caitlin
          p Crafts and creations designed to entice
          // Nav
          nav#nav
            ul
              li
                a.icon.fa-home(href='index.html')
                  span Home
              li
                a.icon.fa-shopping-cart(href='#')
                  span Shop
                ul
                  li
                    a(href='#') All
                  li
                    a(href='#') Kitchen
                  li
                    a(href='#') Home
                  li
                    a(href='#') Garden
                  li
                    a(href='#') Other
              li
                a.icon.fa-info(href='#')
                  span About
              li
                a.icon.fa-retweet(href='#')
                  span Stuff
              li
                a.icon.fa-phone(href='#')
                  span Connect


      // Features
      #features-wrapper
        header
          h2 Header
        section#features.container
          .row
            each product, i in products
              if(product.featured && i < 3)
                .4u(class='12u(mobile)')
                  // Feature
                  section
                    a.image.featured(href='#')
                      img(src='#{product.images[0].src}', alt='')
                    header
                      h3 #{product.title}
                    p #{product.blurb}
          ul.actions
            li
              a.button.icon.fa-file(href='#') See All
              
      // Banner
      #banner-wrapper
        .inner
          section#banner.container
            p
              | Quality creations 
              strong for your home and Family
              | .

      // Main
      #main-wrapper
        #main.container
          .row
            // Content
            #content.8u(class='12u(mobile)')
              // Post
              article.box.post
                header
                  h2
                    a(href='#')
                      span Welcome to Creations By Caitlin 
                      div
                        strong Im Caitlin
                a.image.featured(href='#')
                  img(src='src/images/family.jpg', alt='', style="transform: scaleX(-1);")
                h3 Meet the family
                p
                  | My name is Caitlin, iâ€™m a wife and a mother of one. I love crafting and teaching my little one. My husband and i have been creating these crafts for the past 3 years we love
                p Lorem ipsum dolor sit amet, consectetur adipisicing elit. Rem similique dolores, eligendi illo consequatur, dolor maiores? Laudantium corrupti distinctio, odit dolores ullam sit quaerat, laboriosam nostrum porro numquam esse eos?
                ul.actions
                  li
                    a.button.icon.fa-file(href='#') Continue Reading
            // Sidebar
            #sidebar.4u(class='12u(mobile)')
              // Excerpts
              section
                ul.divided
                  each product, i in products
                    if(product.featured)
                      li
                        // Excerpt
                        article.box.excerpt
                          header
                            h3
                              a(href='#') #{product.title}
                          a.image.fit(href='#')
                            img(src='#{product.images[0].src}', alt='')
      // Footer
      #footer-wrapper
        #footer.container
          header
            h2
              | Questions or comments? 
              strong Get in touch:
          .row
            .6u(class='12u(mobile)')
              section
                form(method='post', action='#')
                  .row(class='50%')
                    .6u(class='12u(mobile)')
                      input(name='name', placeholder='Name', type='text')
                    .6u(class='12u(mobile)')
                      input(name='email', placeholder='Email', type='text')
                  .row(class='50%')
                    .12u
                      textarea(name='message', placeholder='Message')
                  .row(class='50%')
                    .12u
                      a.form-button-submit.button.icon.fa-envelope(href='#') Send Message
            .6u(class='12u(mobile)')
              section
                p
                  | Erat lorem ipsum veroeros consequat magna tempus lorem ipsum consequat Phaselamet
                  | mollis tortor congue. Sed quis mauris sit amet magna accumsan tristique. Curabitur
                  | leo nibh, rutrum eu malesuada.
                .row
                  .6u(class='12u(mobile)')
                    ul.icons
                      li.icon.fa-home
                        | 1234 Somewhere Road
                        br
                        | 													Nashville, TN 00000
                        br
                        | 													USA
                      li.icon.fa-phone
                        | (000) 000-0000
                      li.icon.fa-envelope
                        a(href='#') info@untitled.tld
                  .6u(class='12u(mobile)')
                    ul.icons
                      li.icon.fa-twitter
                        a(href='#') @untitled-tld
                      li.icon.fa-instagram
                        a(href='#') instagram.com/untitled-tld
                      li.icon.fa-dribbble
                        a(href='#') dribbble.com/untitled-tld
                      li.icon.fa-facebook
                        a(href='#') facebook.com/untitled-tld
        #copyright.container
          ul.links
            li &copy; Untitled. All rights reserved.
            li
              | Design: 
              a(href='http://html5up.net') HTML5 UP
